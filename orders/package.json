{
  "name": "orders",
  "version": "1.0.0",
  "description": "Order management service for checkout system",
  "main": "index.js",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "start": "node dist/server.js",
    "build": "tsup src/server.ts --format esm",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:e2e": "vitest run --config vitest.config.e2e.ts",
    "test:e2e:watch": "vitest --config vitest.config.e2e.ts",
    "migrate": "knex migrate:latest --knexfile knexfile.js",
    "migrate:rollback": "knex migrate:rollback --knexfile knexfile.ts",
    "migrate:make": "knex migrate:make --knexfile knexfile.ts -x ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.9.0",
  "devDependencies": {
    "@biomejs/biome": "^2.3.13",
    "@testcontainers/postgresql": "^11.11.0",
    "@types/amqplib": "^0.10.8",
    "@types/pg": "^8.16.0",
    "biome": "^0.3.3",
    "testcontainers": "^11.11.0",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vite-tsconfig-paths": "^6.0.5",
    "vitest": "^4.0.18"
  },
  "dependencies": {
    "amqp-connection-manager": "^5.0.0",
    "amqplib": "^0.10.9",
    "dotenv": "^17.2.3",
    "fastify": "^5.7.2",
    "knex": "^3.1.0",
    "pg": "^8.17.2",
    "tsup": "^8.5.1",
    "uuidv7": "^1.1.0",
    "zod": "^4.3.6"
  }
}
